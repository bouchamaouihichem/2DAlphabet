{
  "NAME": "ZH_Hto4b_ZtoMuMu",
  "PROCESSES": {
    "data_obs": {
      "SYSTEMATICS":[],
      "SCALE": 1.0,
      "COLOR": 1,
      "TYPE": "DATA",
      "comment": "for now, we are just using 2018 samples & bkg sim, so just use Data_18 - not Data_Run2",
      "ALIAS": "data_2018",
      "TITLE": "Data",
      "LOC": "path/FILE:HIST"
    },
    "SIGNAME": {
      "SYSTEMATICS":["lumi"],
      "SCALE": 1.0,
      "COLOR": 1,
      "TYPE": "SIGNAL",
      "TITLE": "m_{X} = 125, m_{Y} = 55 (GeV)",
      "LOC":"path/FILE:HIST"
    }
  },

  "REGIONS": {
    "CR_fail": {
      "PROCESSES": ["SIGNAME"],
      "BINNING": "default"
    },
    "CR_loose": {
      "PROCESSES": ["SIGNAME"],
      "BINNING": "default"
    },
    "CR_pass": {
      "PROCESSES": ["SIGNAME"],
      "BINNING": "default"
    }
  },

  "GLOBAL": {
    "FILE": "ZH_H4b_Zmumu_$process.root",
    "HIST": "hMH_vs_MPnetA_$region",
    "path": "/afs/cern.ch/user/h/hboucham/work/H4B/CMSSW_13_1_0_pre4/src/demo-luna-framework/histo/plots_mu/root",
    "SIGNAME": ["SUSY_ZH_M-55"]
  },

  "BINNING": {
    "default": {
      "X": {
        "NAME": "MASS_H",
        "TITLE": "m_{H} (GeV)",
        "MIN": 0,
        "MAX": 300,
        "NBINS": 30
      },
      "Y": {
        "NAME": "MASS_A",
        "TITLE": "m_{A} (GeV)",
        "MIN": 0,
        "MAX": 70,
        "NBINS": 35
      }
    }
  },

  "SYSTEMATICS": {
    "lumi": {
      "CODE": 0,
      "VAL": 1.016
    }
  },

  "OPTIONS": {
    "plotPrefitSigInFitB": True,
    "plotUncerts": False,
    "plotTitles": False,
    "year": 1
  }
}
